{
  "family": "my-ecs-task",
  "containerDefinitions": [
    {
      "name": "my-app",
      "image": "${AWS_ECR_REPOSITORY_URI}:latest",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ]
}
